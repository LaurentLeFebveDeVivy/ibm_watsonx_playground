# ── watsonx.ai ────────────────────────────────────────────────────────────
WATSONX_URL=https://us-south.ml.cloud.ibm.com
WATSONX_APIKEY=your_api_key_here
WATSONX_PROJECT_ID=your_project_id_here

# ── IBM Cloud Object Storage ─────────────────────────────────────────────
COS_ENDPOINT=https://s3.us-south.cloud-object-storage.appdomain.cloud
COS_API_KEY=your_cos_api_key_here
COS_INSTANCE_ID=your_cos_instance_id_here
COS_BUCKET_NAME=your_bucket_name_here

# ── Milvus (watsonx.data) ────────────────────────────────────────────────
MILVUS_HOST=your_milvus_host_here
MILVUS_PORT=19530
MILVUS_TOKEN=your_milvus_token_here
MILVUS_COLLECTION=default

# ── Database (PostgreSQL via IBM Cloud Databases, or SQLite fallback) ────
DATABASE_URL=sqlite:///watsonx_agent.db
